rder-green{\n    border: 1px solid #39a900;\n}\n.img-profesionales{\n    width: 200px;\n    height: 150px;\n    object-fit: cover;\n}\n.titulos{\n    font-family:'Times New Roman', Times, serif;\n}\n\n.titulo-objetivo{\n    width: 100%;\n    max-height: 450px;\n    background-size:cover;\n}\n.img-obj{\n    width: 100%;\n    height: 100%;\n    object-fit: cover;\n}\n\n.text-titulo{\n    top: 24%;\n}\n.img-titulo-fondo{\n    height: 200px;\n    object-fit: cover;\n    opacity: 0.2;\n}\n.contenedor-actividades{\nwidth: 400px;\nmin-width: 400px;\n}\n.shadow-black{\n    box-shadow: 6px 7px 24px 3px rgba(0,0,0,0.73);\n}\n.shadow-black:hover{\n    transform:scale(1.06);\n}\n.contenedor-icons-contact{\n    width: 250px;\n    min-width: 250px;  \n}\n@media (max-width:992px){\n    \n.user{\n    margin-left: 0px !important;\n}\n.img-slider{\n    /* max-height: 28rem; */\n    height: 350px;\n    width: 100%;\n    object-fit:cover ;\n    \n}\n.nosotros{\n    width: 100% !important;\n    margin-right: 15px;\n    text-align: center;\n    display: flex !important;\n    justify-content: center;\n    align-items: center;\n    \n}\n.text-titulo{\n    top: 34%;\n}\n\n}\n@media(max-width: 662px){\n    .img-logo{\n        max-height: 4rem;\n        display: none;\n    }\n    .img-logo-mini{\n        max-height: 4rem;\n        display: flex !important;\n    }\n}\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}